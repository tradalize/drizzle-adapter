{
  "name": "@tradalize/drizzle-adapter",
  "version": "0.2.1",
  "description": "Drizzle adapter for Tradelize applicantion",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./pg": "./dist/pg/index.js"
  },
  "scripts": {
    "build": "rimraf dist/* && tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "release": "npm run build && changeset publish",
    "format": "prettier . --write",
    "lint": "eslint . --fix && npm run format",
    "lint:ci": "eslint ./src && prettier ./src --check"
  },
  "dependencies": {
    "@tradalize/core": "^0.2.0",
    "drizzle-orm": "^0.29.3",
    "postgres": "^3.4.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/ui": "^1.1.0",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.1.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "drizzle",
    "postgresql",
    "tradalize",
    "algo",
    "backtesting"
  ],
  "author": "Vladyslav Bezsmertnyi",
  "license": "MIT"
}
